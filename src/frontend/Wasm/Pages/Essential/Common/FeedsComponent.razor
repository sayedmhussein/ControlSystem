<MudTable Items="@Feeds" Hover="true" Breakpoint="Breakpoint.Sm" Loading="@IsLoading" LoadingProgressColor="Color.Info">
    <HeaderContent>
        <MudTh>Date/Time</MudTh>
        <MudTh>Subject</MudTh>
    </HeaderContent>
    <RowTemplate>
        <MudTd DataLabel="Nr">@context.FeedTs</MudTd>
        <MudTd DataLabel="Sign" href="@context.FeedUrl">@context.FeedString</MudTd>
    </RowTemplate>
</MudTable>

@code {
    [Parameter]
    public IEnumerable<HomeFeedModel> Feeds { get; set; }
    
    [Parameter]
    public bool IsLoading { get; set; }
}